{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "comma-dangle": ["error", "never"],
        "arrow-parens": ["error", "as-needed"],
        "class-methods-use-this": 0,
        "max-classes-per-file": 0
    }
}
